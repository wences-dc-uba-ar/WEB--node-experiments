
# create DB bpm

	mysql -ubpm -pbpm  -e 'CREATE DATABASE bpm CHARACTER SET utf8 COLLATE utf8_general_ci'

# dumpit for the record

	mysqldump -ubpm -pbpm --complete-insert --skip-extended-insert --skip-dump-date --routines bpm >bpm.sql

# commit for the record

	git add bpm.sql
	git add bpm.log
	git commit -m "added empty db dump"

# sync model->DB and dump again

	mysqldump -ubpm -pbpm --complete-insert --skip-extended-insert --skip-dump-date --routines bpm >bpm.sql
